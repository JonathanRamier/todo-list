{
    "private": true,
    "name": "test-productman",
    "version": "0.0.0",
    "description": "",
    "author": "Nicolas Panel",
    "scripts": {
        "ng": "ng",
        "pretest": "if [ ! -d dist ]; then yarn dev:build; fi",
        "test": "yarn test:server && yarn test:client -- --single-run",
        "lint": "yarn lint:client && yarn lint:server",
        "lint:client": "ng lint && stylelint client/**/*.scss",
        "lint:server": "autopep8 --in-place server/**/*.py && flake8 server",
        "test:all": "yarn lint && yarn test",
        "test:client": "ng test",
        "test:server": "python manage.py check && pytest server -vv",
        "dev:start": "concurrently \"yarn dev:server\" \"yarn dev:build -- --watch\" \"yarn dev:wds\"",
        "dev:build": "ng build --no-progress --deploy-url http://localhost:4200 --base-href=http://localhost:4200/",
        "dev:wds": "ng serve --hmr --disable-host-check --no-progress --proxy-config proxy.conf.json",
        "dev:server": "python manage.py runserver 4300",
        "precommit": "yarn lint",
        "prepush": "yarn test:all",
        "postmerge": "yarn && pip install -r requirements.dev.txt && python manage.py migrate",
        "preversion": "yarn test:all"
    },
    "dependencies": {
        "@angular/cli": "^1.2.6",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/compiler-cli": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angularclass/hmr": "^2.1.3",
        "@ngrx/effects": "^4.0.5",
        "@ngrx/store": "^4.0.3",
        "@ngrx/store-devtools": "^4.0.0",
        "@types/jasmine": "2.5.38",
        "@types/node": "~6.0.60",
        "bootstrap-sass": "3.3.7",
        "codelyzer": "2",
        "concurrently": "^3.5.0",
        "core-js": "^2.4.1",
        "husky": "^0.14.3",
        "jasmine-core": "~2.5.2",
        "jasmine-spec-reporter": "~3.2.0",
        "karma": "~1.4.1",
        "karma-chrome-launcher": "~2.0.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^0.2.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "node-sass": "^4.5.3",
        "rxjs": "~5.4.0",
        "stylelint": "^8.0.0",
        "stylelint-config-standard": "^17.0.0",
        "stylelint-scss": "^2.0.1",
        "ts-node": "~2.0.0",
        "tslint": "4",
        "tslint-config-airbnb": "^5.3.0",
        "typescript": "~2.4.0",
        "zone.js": "0.8.16"
    },
    "engines": {
        "node": "8.6.x"
    }
}
